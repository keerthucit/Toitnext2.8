	<queryString><![CDATA[select locations.name as warehouse,categories.name as Itcategory,products.itemcode,products.pricebuy as costprice, products.name,stockcurrent.units as CInventory,case  when products.pricebuy*stockcurrent.units<0 then 0 else products.pricebuy*stockcurrent.units end as Value from stockcurrent
left join locations on locations.id=stockcurrent.location 
left join products on stockcurrent.product=products.id
left join categories on products.category = categories.id
where stockcurrent.units>0]]></queryString>

