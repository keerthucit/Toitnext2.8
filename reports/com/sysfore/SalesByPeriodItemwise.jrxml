	<parameter name="ARG" isForPrompting="false" class="java.lang.Object"/>
	<queryString><![CDATA[SELECT  CTG.NAME, P.CODE, P.NAME AS PRODUCT,
SUM(TL.UNITS) AS QTY, TL.PRICE AS MRP ,(SUM(TL.UNITS)*TL.PRICE)- SUM(TL.DISCOUNTPRICE) AS VALUE, SUM(TL.DISCOUNTPRICE) AS DISCOUNT
FROM RECEIPTS 	
	INNER JOIN TICKETS USING(ID) 
	LEFT JOIN TICKETLINES  AS TL ON RECEIPTS.ID=TL.TICKET
	LEFT JOIN  PRODUCTS AS P ON P.ID = TL.PRODUCT
	INNER JOIN CATEGORIES AS CTG ON P.CATEGORY = CTG.ID
WHERE  TICKETS.ISCANCELTICKET='N'
GROUP BY CTG.NAME, P.CODE, P.NAME
ORDER BY CTG.NAME, P.CODE,P.NAME]]></queryString>

